<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd" id="WebApp_ID" version="3.1">
  <display-name>Servlet2Servlet</display-name>
  
  <servlet>
    <servlet-name>s1</servlet-name>
    <servlet-class>Option_servlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>s1</servlet-name>
    <url-pattern>/option_form</url-pattern>
  </servlet-mapping>
  
  
  <servlet>
    <servlet-name>s2</servlet-name>
    <servlet-class>ReqDpatcher</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>s2</servlet-name>
    <url-pattern>/RD</url-pattern>
  </servlet-mapping>
  
  
   <servlet>
    <servlet-name>s3</servlet-name>
    <servlet-class>SendRdirect</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>s3</servlet-name>
    <url-pattern>/Square</url-pattern>
  </servlet-mapping>
  
</web-app>